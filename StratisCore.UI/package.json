{
  "name": "straxui",
  "description": "STRAX Wallet",
  "version": "1.1.0",
  "author": {
    "name": "Stratis Group Ltd.",
    "email": "support@stratisplatform.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/stratisproject/StraxUI",
  "keywords": [
    "fullnode",
    "ui",
    "stratis",
    "core",
    "bitcoin",
    "angular",
    "electron",
    "typescript",
    "scss",
    "bootstrap"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "node ./src/environments/git-version.js && electron-builder install-app-deps",
    "ng": "ng",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "start": "npm run && npm-run-all -p ng:serve \"electron:serve -- {@}\" --",
    "mainnet": "npm start --",
    "testnet": "npm run && npm-run-all -p ng:serve \"electron:serve:testnet -- {@}\" --",
    "sidechain": "npm run && npm-run-all -p ng:serve \"electron:serve:sidechain -- {@}\" --",
    "sidechain:testnet": "npm run && npm-run-all -p ng:serve \"electron:serve:sidechain:testnet -- {@}\" --",
    "build": "npm run && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:serve:testnet": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve --testnet",
    "electron:serve:sidechain": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve --sidechain",
    "electron:serve:sidechain:testnet": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve --sidechain --testnet",
    "electron:local": "npm run build:prod && electron .",
    "package:linux": "npm run build:prod && electron-builder build --linux",
    "package:linuxarm": "npm run build:prod && electron-builder build --linux --armv7l",
    "package:windows": "npm run build:prod && electron-builder build --windows",
    "package:windows86": "npm run build:prod && electron-builder build --windows --ia32",
    "package:windows64": "npm run build:prod && electron-builder build --windows --x64",
    "package:mac": "npm run build:prod && electron-builder build --mac",
    "package:sidechain:linux": "npm run build:prod && electron-builder build --linux --config ./electron-sidechain-builder.json",
    "package:sidechain:linuxarm": "npm run build:prod && electron-builder build --linux --armv7l --config ./electron-sidechain-builder.json",
    "package:sidechain:windows": "npm run build:prod && electron-builder build --windows --config ./electron-sidechain-builder.json",
    "package:sidechain:windows86": "npm run build:prod && electron-builder build --windows --ia32 --config ./electron-sidechain-builder.json",
    "package:sidechain:windows64": "npm run build:prod && electron-builder build --windows --x64 --config ./electron-sidechain-builder.json",
    "package:sidechain:mac": "npm run build:prod && electron-builder build --mac --config ./electron-sidechain-builder.json",
    "test": "ng test --watch=false",
    "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/cdk": "11.0.2",
    "@aspnet/signalr": "1.1.4",
    "electron-context-menu": "2.3.0",
    "minimist": "1.2.5",
    "ngx-pagination": "5.0.0",
    "@techiediaries/ngx-qrcode": "9.1.0",
    "ngx-snackbar": "2.0.1",
    "tslib": "2.0.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular-eslint/builder": "0.8.0-beta.5",
    "@angular/animations": "11.0.4",
    "@angular/cli": "^11.1.4",
    "@angular/common": "11.0.4",
    "@angular/compiler": "11.0.4",
    "@angular/compiler-cli": "11.0.4",
    "@angular/core": "11.0.4",
    "@angular/forms": "11.0.4",
    "@angular/language-service": "11.0.4",
    "@angular/platform-browser": "11.0.4",
    "@angular/platform-browser-dynamic": "11.0.4",
    "@angular/router": "11.0.4",
    "@ng-bootstrap/ng-bootstrap": "8.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.0.4",
    "@types/node": "12.12.6",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "bootstrap-scss": "4.5.3",
    "chai": "4.2.0",
    "codelyzer": "6.0.1",
    "core-js": "3.8.1",
    "electron": "11.1.0",
    "electron-builder": "22.9.1",
    "electron-reload": "1.5.0",
    "eslint": "7.13.0",
    "eslint-plugin-import": "2.22.1",
    "git-describe": "4.0.4",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "~5.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.1",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.1.2",
    "ngx-bootstrap": "6.2.0",
    "ngx-clipboard": "14.0.1",
    "ngx-electron": "2.2.0",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.3",
    "spectron": "13.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.0.5",
    "wait-on": "^5.2.1",
    "webdriver-manager": "12.1.7",
    "zone.js": "0.10.3"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
